{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "routes": [
    {
      "src": "pages/register.js",
      "dest": "/register"
    },
    {
      "src": "pages/login.js",
      "dest": "/login"
    },
    {
      "src": "pages/dashboard.js",
      "dest": "/dashboard"
    },
    {
      "src": "pages/index.js",
      "dest": "/"
    },
    {
      "src": "pages/signup.js",
      "dest": "/signup"
    }
  ]
}
